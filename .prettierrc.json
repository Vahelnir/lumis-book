{
  "$schema": "https://json.schemastore.org/prettierrc",
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^vue(\\/.*)?$",
    "^vue-(.*?)(\\/.*)?$",
    "^@vue(\\/.*)?$",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@\\/(?!.*(\\.vue))(.*)$",
    "",
    "^@\\/(.*).vue$",
    "^(?!@\\/)(.*)\\.vue$",
    "",
    "^[.]"
  ],
  "importOrderTypeScriptVersion": "5.0.0",
  "tailwindFunctions": ["tw"]
}
